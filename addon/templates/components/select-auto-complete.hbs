{{#auto-complete value=value
                 multiSelect=multiSelect
                 content=content
                 updated=updated
                 name=name
                 onQueryChange=(fn this.queryChange)
                 doubleClickItem=doubleClickItem
                 optionLabelPath=optionLabelPath
                 groupLabelPath=groupLabelPath
                 prompt=prompt
                 disableClear=disableClear
                 disableSearch=disableSearch
                 disabled=disabled
                 as |item|}}
  {{#if hasBlock}}
    {{yield item}}
  {{else}}
    {{get item optionLabelPath}}
  {{/if}}
{{/auto-complete}}
