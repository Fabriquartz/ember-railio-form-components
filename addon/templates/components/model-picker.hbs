{{auto-complete content=content
                multiSelect=multiSelect
                enableSelectAll=enableSelectAll
                _selectAll=_selectAll
                value=(if _selectAll emptyValue value)
                onQueryChange=(if lookupOnSearch (perform lookupModel))
                optionLabelPath=optionLabelPath
                groupLabelPath=groupLabelPath
                prompt=(if _selectAll 'Selected all' prompt)
                name=name
                updated=(action 'update')
                allowClear=allowClear
                disabled=disabled}}
