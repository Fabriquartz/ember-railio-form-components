    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.28.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-qunit": "~4.4.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^4.0.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~3.14.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-font-awesome": "3.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-proxy-util": "0.0.16",
    "ember-qunit": "^5.1.4",
    "ember-railio-formatting": "0.0.27",
    "ember-resolver": "^8.0.2",
    "ember-source": "3.28.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-ember-suave": "1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "sass": "^1.23.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "Fabriquartz/ember-railio-form-components",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "resolutions": {
    "ember-concurrency": "1.1.3"
  },
  "dependencies": {
    "ember-cli-babel": "7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-sass": "10.0.1",
    "ember-concurrency": "^1.1.0",
