{{paper-input id=inputElementId
              class=class
              label=label
              value=_value
              onChange=(action "changed")
              textarea=textarea
              placeholder=placeholder
              type="text"
              disabled=disabled
              autofocus=autofocus
              onfocus=onFocus
              onblur=onBlur
